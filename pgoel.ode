#pgoel
qsto'=-ksto*(qsto)
qgut'=ksto*qsto-kgut*qgut
G'=R0-(EG0+SI*I)*G+kgut*qgut
I'=Imax*(G^2/(alpha+G^2))-kI*I
global -1 pp-t {qsto=30}
# it will set the value of qsto to 30 when pp-t becomes negative (or when t reaches pp)
init qsto=0,G=70.855,I=9.359,qgut=0
par ksto=0.036,kgut=0.098,R0=2.1,EG0=0.001,SI=0.00306,Imax=0.28,alpha=10000,kI=0.01,pp=100
@ xp=t,yp=G,xlo=0,xhi=500,ylo=60,yhi=90,total=500,dt=0.001
done
